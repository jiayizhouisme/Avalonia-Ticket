<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="GetStartedApp.App"
             xmlns:local="using:GetStartedApp"
             xmlns:fluent="using:Avalonia.Themes.Fluent"
             RequestedThemeVariant="Default">
	<!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

	<Application.DataTemplates>
		<local:ViewLocator/>
	</Application.DataTemplates>

	<Application.Styles>
		<StyleInclude Source="avares://GetStartedApp/Controls/Style/TestControlStyle.axaml"/>
		<StyleInclude Source="avares://LayUI.Avalonia/Themes/Default.axaml" />
		<fluent:FluentTheme />
	</Application.Styles>
	
	
</Application>